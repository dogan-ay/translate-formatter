{
    "manifest_version": 3,
    "name": "Google Translate Formatter",
    "version": "1.1",
    "description": "test version",
    "action":{
        "default_popup": "index.html",
        "default_icon": {
            "128": "icon.png"
        }

    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
          "matches": ["https://*/*"],
          "js": ["scripts/formatter.js"]
        }
    ]
  }